{
  "framework": null,
  "ignoreCommand": "exit 1",
  "buildCommand": "cd frontend && yarn build",
  "installCommand": "cd frontend && yarn install && cd .. && cd api && yarn install",
  "ignoreCommand": "exit 1",
  "outputDirectory": "frontend/build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}