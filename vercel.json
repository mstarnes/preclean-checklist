{
  "framework": null,
  "buildCommand": "yarn workspace frontend build",
  "outputDirectory": "frontend/build",
  "installCommand": "yarn install",
  "ignoreCommand": "git diff HEAD --quiet .",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/server.js" },
    { "src": "/.*", "dest": "/frontend/build/index.html" }
  ]
}